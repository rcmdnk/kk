# source sentaku
if type sentaku >& /dev/null;then
  . sentaku -n
else
echo "Install sentaku (https://github.com/rcmdnk/sentaku) first."
  exit 1
fi

. sentaku -n

_SENTAKU_SEPARATOR=$'\n'

_sf_execute () { # {{{
  :
} # }}}

_sf_select () { # {{{
  kill -kill $(echo ${_s_inputs[$_s_current_n]}|awk '{print $1}')
  _sf_remove
} # }}}

_sf_d () {
  _sf_remove
}

ps -u$USER -o pid,command|grep -v "ps -u$USER -o pid,command"|grep -v kk|grep -v sentaku|_sf_main
